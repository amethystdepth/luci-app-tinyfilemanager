{
  "luci-app-tinyfilemanager": {
    "description": "Grant access to tinyfilemanager procedures",
    "read": {
      "file": {
        "/usr/libexec/tinyfilemanager-update": [ "exec" ],
        "/etc/init.d/tinyfilemanager": [ "exec" ],
        "/var/tinyfilemanager/releaseslist": [ "read" ]
      },
      "uci": [ "tinyfilemanager" ]
    },
    "write": {
      "uci": [ "tinyfilemanager" ]
    }
  }
}
